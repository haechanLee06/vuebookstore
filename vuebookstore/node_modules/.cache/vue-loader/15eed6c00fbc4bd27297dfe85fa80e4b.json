ngth=\"20\" clearable placeholder=\"请输入账号/手机号/邮箱\" />\n                </el-form-item>\n                <el-form-item label=\"密码\" size=\"large\" prop=\"pass\">\n                    <el-input v-model=\"form.pass\" size=\"large\" maxlength=\"20\" type=\"password\" show-password clearable placeholder=\"请输入密码\" />\n                </el-form-item>\n                <el-form-item label=\"确认\" size=\"large\" prop=\"ck_pass\">\n                    <el-input v-model=\"form.ck_pass\" size=\"large\" maxlength=\"20\" type=\"password\" show-password clearable placeholder=\"请确认密码\" />\n                </el-form-item>\n                <el-form-item>\n                    <slot name=\"cut\"></slot>\n                </el-form-item>\n                <el-form-item>\n                    <el-button type=\"warning\" size=\"large\" style=\"width: 100%\" @click=\"onSubmit('form')\">注册</el-button>\n                </el-form-item>\n            </el-form>\n        </div>\n    </div>\n</template>\n\n<script>\nimport { ElMessage } from 'element-plus'\n\nexport default {\n    name: 'Register',\n    data() {\n        return {\n            form: {\n                name: '',\n                pass: '',\n                ck_pass: ''\n            },\n            rules: {\n                name: [\n                    {\n                        required: true,\n                        message: '账号不能为空',\n                        trigger: 'change'\n                    }\n                ],\n                pass: [\n                    {\n                        required: true,\n                        message: '密码不能为空',\n                        trigger: 'change'\n                    }\n                ],\n                ck_pass: [\n                    {\n                        required: true,\n                        validator: this.validatePass2,\n                        trigger: 'change'\n                    }\n                ]\n            }\n        }\n    },\n    methods: {\n        validatePass2(rule, value, callback) {\n            if (value === '') {\n                callback(new Error('确认密码不能为空'))\n            } else if (value !== this.form.pass) {\n                callback(new Error('2次输入的密码不同！请重新输入'))\n            } else {\n                callback()\n            }\n        },\n        onSubmit(formName) {\n            this.$refs[formName].validate((valid) => {\n                if (valid) {\n                    ElMessage({\n                        message: '注册成功！',\n                        type: 'success',\n                        duration: 1000,\n                        onClose: () => {\n                            this.$emit('success', true)\n                        }\n                    })\n                } else {\n                    return false\n                }\n            })\n        }\n    }\n}\n</script>\n\n<style scoped lang=\"scss\">\n.form-box :deep(.el-form-item__label) {\n    color: white;\n}\n</style>\n"]}]}